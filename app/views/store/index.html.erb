<% cache ['store', Product.latest] do%>
	<% @products.each do |product|  %>
	<%cache ['entry', product] do %>
	<div class="entry">
	<%= product.name %>
	<%= product.description %>
	<%= number_to_currency(product.price) %>
	<%= button_to 'Add to cart', line_items_path(product_id: product) %>
	</div>
	<% end %>
	<% end %>
<% end %>